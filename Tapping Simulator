function unlockmaps()

    local part = Instance.new("Part")
    part.Parent = game.Workspace
    part.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame
    part.Anchored = true
    part.CanCollide = falsed
    part.Transparency = .5
    
    wait(.1)
    
    local args = {
        [1] = CFrame.new(-163.98446655273438, 987.5964965820312, -403.33941650390625) * CFrame.Angles(-0, 0, -0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-179.51564025878906, 1524.6309814453125, -682.4987182617188) * CFrame.Angles(3.1415927410125732, -0, -3.1415927410125732)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-264.35888671875, 2123.06298828125, -694.6613159179688) * CFrame.Angles(3.1415927410125732, -0, -3.1415927410125732)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-201.77197265625, 2944.481689453125, -470.40875244140625) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-166.28515625, 4235.38916015625, -779.1531372070312) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-276.2012634277344, 5425.62158203125, -480.28778076171875) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-243.2586669921875, 8376.5166015625, -483.8897705078125) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-253.30348205566406, 11262.8818359375, -237.8885955810547) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-511.0211181640625, 13812.0322265625, -543.2418212890625) * CFrame.Angles(-0, -1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    wait(3)
    
    local args = {
        [1] = CFrame.new(-408.697509765625, 17180.138671875, -552.0464477539062) * CFrame.Angles(0, 1.5707963705062866, 0)
    }
    
    game:GetService("ReplicatedStorage").Events.Teleport:FireServer(unpack(args))
    
    
    
    wait(3)
    
    game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = part.CFrame
    
    part:Destroy()    

    getgenv().unlockingmap = false
    
end

    

            

local Tab = Window:MakeTab({
	Name = "Auto",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "AutoTap",
	Default = false,
	Callback = function(t)
        _G.autoclicker = t
        while _G.autoclicker == true do
            game:GetService("ReplicatedStorage").Events.Tap:FireServer()
        wait()
        end
	end    
})

Tab:AddToggle({
    Name = "Auto Claim Rank Reward",
    Default = false,
    Callback = function(Value)
    if Value == true then
    getgenv().autoclaimrankreward = true
    else
    getgenv().autoclaimrankreward = false
    end
    while getgenv().autoclaimrankreward == true do      
    claimrankreward()
    wait()
    end
    end    
    })
    
    Tab:AddToggle({
        Name = "Auto Claim Random Reward",
        Default = false,
        Callback = function(Value)
        if Value == true then
        getgenv().autoclaimrandomreward = true
        else
        getgenv().autoclaimrandomreward = false
        end
        while getgenv().autoclaimrandomreward == true do      
        claimrandomreward()
        wait()
        end
        end    
        })

        local Section = Tab:AddSection({
            Name = "Other"
        })

        Tab:AddDropdown({
            Name = "Dropdown",
            Default = "1",
            Options = {"1","5","20","100","500","2000","4500","8000","12500","18000","24500","32000","40500","50000","1000000"},
            Callback = function(t)
                getgenv().ReAmount = t
            end    
        })

        
        Tab:AddToggle({
            Name = "Auto Rebirth",
            Default = false,
            Callback = function(t)
                _G.autorebirth = t
                while _G.autorebirth == true do wait()
                    local args = {[1] = getgenv().ReAmount}
                        game:GetService("ReplicatedStorage").Events.Rebirth:FireServer(unpack(args))
                    wait()
                end
            end   
        })




