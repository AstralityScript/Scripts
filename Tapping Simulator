	Name = "Redeem Codes",
	Callback = function()
redeemcodes()
  	end    
})

Tab:AddButton({
	Name = "Unlock Maps",
	Callback = function()

if getgenv().unlockingmap == false then

        getgenv().unlockingmap = true
        notice("Please Wait", "Unlocking...")
      	unlockmaps()

else

    print("Running")
    notice("Please Wait", "Unlocking...")

end
  	end    
})


